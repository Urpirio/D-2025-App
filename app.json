{
  "expo": {
    "name": "D-2025-App",
    "slug": "D-2025-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/IconLogo/IconoApp.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/IconLogo/IconoApp.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/IconLogo/IconoApp.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.urpiriodev.D2025App"
    },
    "web": {
      "favicon": "./assets/IconLogo/IconoApp.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "26877314-d692-4cef-8031-42d80eec4264"
      }
    }
  }
}
